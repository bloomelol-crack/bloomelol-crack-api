{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "esModuleInterop": true,
    "types": [
      "node"
    ],
    "outDir": "dist",
    "rootDir": "./",
    "baseUrl": "./src",
    "paths": {
      "fol/*": [
        "fol/*"
      ],
      "env/*": [
        "env/*"
      ]
    },
    "plugins": [
      {
        "transform": "typescript-transform-paths"
      }
    ]
  },
  "exclude": [
    "node_modules",
    "src2"
  ]
}